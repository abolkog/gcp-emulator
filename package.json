{
  "name": "gcp-emulators",
  "version": "1.0.0",
  "description": "emulator and config for running GCP local dev",
  "main": "index.js",
  "scripts": {
    "start": "FIRESTORE_EMULATOR_HOST=[::1]:8080 concurrently \"npm run seed\" \"npm run emulators\"",
    "seed": "node src/index.js",
    "emulators": "firebase emulators:start --project kopa-ai-local",
    "init": "firebase init emulators ",
    "firebase": "firebase",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Khalid Elshafie",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/firestore": "^7.3.0",
    "concurrently": "^8.2.2",
    "firebase-tools": "^13.1.0"
  }
}
